class Solution {
    public void sortColors(int[] nums) {
        int i=0, j=0, k=nums.length-1;
        while(j<=k){
            if(nums[j] == 0)
            swap(nums, i++, j++);

            else if (nums[j] == 2)
            swap(nums, j, k--);

            else j++;
            
        }
        
    }
    private void swap(int[] nums, int p1, int p2){
        int temp = nums[p1];
        nums[p1] = nums[p2];
        nums[p2] = temp;

    }
}
